    <PLCOORDWIN_WorldSpecificLocations>세계의 특정 위치들</PLCOORDWIN_WorldSpecificLocations>  <!-- tool-tip -->

    <!-- translators: be wary that only two letters can fit in the "specific locations" buttons -->
    <PLCOORDWIN_NorthPoleAbbr>북극</PLCOORDWIN_NorthPoleAbbr>  <!-- North Pole -->
    <PLCOORDWIN_NorthPole>북극</PLCOORDWIN_NorthPole>  <!-- tool-tip displayed on mouse over button -->

    <PLCOORDWIN_SouthPoleAbbr>남극</PLCOORDWIN_SouthPoleAbbr>  <!-- south pole -->
    <PLCOORDWIN_SouthPole>남극</PLCOORDWIN_SouthPole>  <!-- tool-tip -->

    <PLCOORDWIN_OriginPointAbbr>O</PLCOORDWIN_OriginPointAbbr>  <!-- 'O' for Origin -->
    <PLCOORDWIN_OriginPoint>원점 [0°, 0°]</PLCOORDWIN_OriginPoint>

    <!-- point located at the opposite of the Origin point, on the other side of the globe; 'Ante' is Latin for 'opposite' -->
    <PLCOORDWIN_AnteOriginAbbr>AO</PLCOORDWIN_AnteOriginAbbr>
    <PLCOORDWIN_AnteOrigin>원점 반대편 [0°, 180°]</PLCOORDWIN_AnteOrigin>


    <!-- {0} is a strictly positive number -->
    <PLCOORDWIN_TileIdNum>타일 ID [0, {0}]:</PLCOORDWIN_TileIdNum>
    <PLCOORDWIN_GoButton>이동!</PLCOORDWIN_GoButton>  <!-- as in "Go to the tile" -->
    <!-- {0} is a tile ID (number); {1} is the maximum tile number (not included in the range!)-->
    <PLCOORDWIN_TileIdOutOfRange>범위 벗어남: {0}; 범위: [0, {1}).</PLCOORDWIN_TileIdOutOfRange>
    <PLCOORDWIN_CoordsLabel>좌표:</PLCOORDWIN_CoordsLabel>  <!-- as in "coordinates" but space is tight -->
    <PLCOORDWIN_NoTileForCoords>주어진 좌표의 타일을 찾을 수 없습니다::</PLCOORDWIN_NoTileForCoords>

    <!--========================================== Coordinates Parser [COORD] =======================================-->
    <!-- coordinates.cs -->
    <PLCOORD_StringDoesntMatchCoords>주어진 문자열이 좌표 문자열이 아닙니다.</PLCOORD_StringDoesntMatchCoords>
    <PLCOORD_FailedParseLongitude>경도 파싱 실패</PLCOORD_FailedParseLongitude>
    <PLCOORD_FailedParseLatitude>위도 파싱 실패</PLCOORD_FailedParseLatitude>
    <PLCOORD_LongitudeRange>경도는 [0, 180] 범위 안이어야 합니다</PLCOORD_LongitudeRange>
    <PLCOORD_LatitudeRange>위도는 [0, 180] 범위 안이어야 합니다</PLCOORD_LatitudeRange>
    <!-- displayed on parsing error: string's followed by a space and a letter which is not 'N' or 'S'; latitude should be either 'N' (north) or 'S' (south) -->
    <PLCOORD_LatitudeDirection>위도 방향은 N이나 S여야 하지만 지금은</PLCOORD_LatitudeDirection>
    <!-- displayed on parsing error: string's followed by a space and a letter which is not 'W' or 'E'; longitude should be either 'W' (west) or 'E' (east) -->
    <PLCOORD_LongitudeDirection>경도 방향은 W나 E여야 하지만 지금은</PLCOORD_LongitudeDirection>

